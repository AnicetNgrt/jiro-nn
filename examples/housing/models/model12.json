{
    "epochs": 100,
    "loss": "MSE",
    "hidden_layers": [
        {
            "out_size": 10,
            "activation": "ReLU",
            "optimizer": {
                "Momentum": {
                    "momentum": 0.9,
                    "learning_rate": {
                        "InverseTimeDecay": {
                            "decay_rate": 1.0,
                            "decay_steps": 1.0,
                            "initial_learning_rate": 0.001
                        }
                    }
                }
            }
        },
        {
            "out_size": 10,
            "activation": "ReLU",
            "optimizer": {
                "Momentum": {
                    "momentum": 0.9,
                    "learning_rate": {
                        "InverseTimeDecay": {
                            "decay_rate": 1.0,
                            "decay_steps": 1.0,
                            "initial_learning_rate": 0.001
                        }
                    }
                }
            }
        },
        {
            "out_size": 10,
            "activation": "ReLU",
            "optimizer": {
                "Momentum": {
                    "momentum": 0.9,
                    "learning_rate": {
                        "InverseTimeDecay": {
                            "decay_rate": 1.0,
                            "decay_steps": 1.0,
                            "initial_learning_rate": 0.001
                        }
                    }
                }
            }
        },
        {
            "out_size": 10,
            "activation": "ReLU",
            "optimizer": {
                "Momentum": {
                    "momentum": 0.9,
                    "learning_rate": {
                        "InverseTimeDecay": {
                            "decay_rate": 1.0,
                            "decay_steps": 1.0,
                            "initial_learning_rate": 0.001
                        }
                    }
                }
            }
        }
    ],
    "final_layer": {
        "activation": "Tanh",
        "optimizer": {
            "Momentum": {
                "momentum": 0.9,
                "learning_rate": {
                    "InverseTimeDecay": {
                        "decay_rate": 1.0,
                        "decay_steps": 1.0,
                        "initial_learning_rate": 0.001
                    }
                }
            }
        }
    },
    "batch_size": 128,
    "folds": 8,
    "dataset": {
        "name": "kc_house_data",
        "features": [
            {
                "name": "lat",
                "filter_outliers": true,
                "with_squared": {
                    "name": "lat^2",
                    "normalized": true
                },
                "normalized": true
            },
            {
                "name": "long",
                "filter_outliers": true,
                "with_squared": {
                    "name": "long^2",
                    "normalized": true
                },
                "normalized": true
            },
            {
                "name": "sqft_living",
                "filter_outliers": true,
                "log10": true,
                "with_squared": {
                    "name": "sqft_living^2",
                    "normalized": true
                },
                "normalized": true
            },
            {
                "name": "sqft_lot",
                "filter_outliers": true,
                "log10": true,
                "with_squared": {
                    "name": "sqft_lot^2",
                    "normalized": true
                },
                "normalized": true
            },
            {
                "name": "sqft_above",
                "filter_outliers": true,
                "log10": true,
                "normalized": true
            },
            {
                "name": "sqft_basement",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "condition",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "grade",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "yr_built",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "yr_renovated",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "bedrooms",
                "filter_outliers": true,
                "log10": true,
                "normalized": true
            },
            {
                "name": "bathrooms",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "floors",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "view",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "waterfront",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "price",
                "filter_outliers": true,
                "normalized": true,
                "log10": true,
                "out": true
            }
        ]
    }
}
