{
    "name": "model1",
    "epochs": 100,
    "dropout": [0.2, 0.4, 0.5],
    "layers": [64, 32, 16],
    "activation": {
        "type": "Tanh"
    },
    "optimizer": {
        "type": "SGD",
        "params": {
            "learning_rate": {
                "type": "Constant",
                "params": 0.1
            }
        }
    },
    "batch_size": 64,
    "folds": 5,
    "dataset": {
        "name": "kc_house_data",
        "features": [
            {
                "name": "lat",
                "extract_normalized": {
                    "name": "lat_norm"
                }
            },
            {
                "name": "long",
                "extract_normalized": {
                    "name": "long"
                }
            },
            {
                "name": "date",
                "date_format": "%Y%m%dT%H%M%S",
                "with_extracted_timestamp": {
                    "name": "timestamp",
                    "normalized": true
                },
                "with_extracted_month": {
                    "name": "month",
                    "log10": true
                },
                "used_in_model": false
            }
        ]
    }
}
