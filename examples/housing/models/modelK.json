{
    "name": "model4",
    "epochs": 100,
    "layers": [3, 3, 3, 3, 3, 3],
    "activation": {
        "type": "Tanh"
    },
    "loss": {
        "type": "MSE"
    },
    "optimizer": {
        "type": "SGD",
        "params": {
            "learning_rate": {
                "type": "Constant",
                "params": 0.001
            }
        }
    },
    "batch_size": 128,
    "folds": 8,
    "dataset": {
        "name": "kc_house_data",
        "features": [
            {
                "name": "lat",
                "normalized": true
            },
            {
                "name": "long",
                "normalized": true
            },
            {
                "name": "date",
                "date_format": "%Y%m%dT%H%M%S",
                "with_extracted_timestamp": {
                    "name": "timestamp",
                    "normalized": true
                },
                "with_extracted_month": {
                    "name": "month",
                    "normalized": true
                },
                "used_in_model": false
            },
            {
                "name": "price",
                "normalized": true,
                "out": true
            }
        ]
    }
}
