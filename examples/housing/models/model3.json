{
    "epochs": 100,
    "loss": "MSE",
    "hidden_layers": [
        {
            "out_size": 9,
            "activation": "ReLU",
            "optimizer": {
                "SGD": {
                    "learning_rate": {
                        "Constant": 0.001
                    }
                }
            }
        }
    ],
    "final_layer": {
        "activation": "Sigmoid",
        "optimizer": {
            "SGD": {
                "learning_rate": {
                    "Constant": 0.001
                }
            }
        }
    },
    "batch_size": 128,
    "folds": 8,
    "dataset": {
        "name": "kc_house_data",
        "features": [
            {
                "name": "lat",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "long",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "sqft_living",
                "filter_outliers": true,
                "log10": true,
                "normalized": true
            },
            {
                "name": "sqft_lot",
                "filter_outliers": true,
                "log10": true,
                "normalized": true
            },
            {
                "name": "sqft_above",
                "filter_outliers": true,
                "log10": true,
                "normalized": true
            },
            {
                "name": "sqft_basement",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "condition",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "grade",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "yr_built",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "yr_renovated",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "sqft_living15",
                "filter_outliers": true,
                "log10": true,
                "normalized": true
            },
            {
                "name": "sqft_lot15",
                "filter_outliers": true,
                "log10": true,
                "normalized": true
            },
            {
                "name": "bedrooms",
                "filter_outliers": true,
                "log10": true,
                "normalized": true
            },
            {
                "name": "bathrooms",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "floors",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "view",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "waterfront",
                "filter_outliers": true,
                "normalized": true
            },
            {
                "name": "price",
                "filter_outliers": true,
                "normalized": true,
                "log10": true,
                "out": true
            }
        ]
    }
}
