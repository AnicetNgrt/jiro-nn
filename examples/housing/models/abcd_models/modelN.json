{
    "name": "model4",
    "epochs": 50,
    "layers": [6, 6, 6, 6, 6, 6],
    "activation": {
        "type": "Tanh"
    },
    "loss": {
        "type": "MSE"
    },
    "optimizer": {
        "type": "SGD",
        "params": {
            "learning_rate": {
                "type": "Constant",
                "params": 0.001
            }
        }
    },
    "batch_size": 128,
    "folds": 8,
    "dataset": {
        "name": "kc_house_data",
        "features": [
            {
                "name": "lat",
                "normalized": true
            },
            {
                "name": "long",
                "normalized": true
            },
            {
                "name": "sqft_living",
                "normalized": true
            },
            {
                "name": "sqft_lot",
                "normalized": true
            },
            {
                "name": "sqft_above",
                "normalized": true
            },
            {
                "name": "sqft_basement",
                "normalized": true
            },
            {
                "name": "price",
                "normalized": true,
                "out": true
            }
        ]
    }
}
