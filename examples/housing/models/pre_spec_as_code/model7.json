{
    "epochs": 50,
    "loss": "MSE",
    "weight_initializer": "GlorotUniform",
    "biases_initializer": "Zeros",
    "hidden_layers": [
        {
            "out_size": 2,
            "activation": "ReLU",
            "optimizer": {
                "Momentum": {
                    "momentum": 0.1,
                    "learning_rate": {
                        "Constant": 0.001
                    }
                }
            }
        }
    ],
    "final_layer": {
        "activation": "Linear",
        "optimizer": {
            "Momentum": {
                "momentum": 0.1,
                "learning_rate": {
                    "Constant": 0.001
                }
            }
        }
    },
    "batch_size": 128,
    "folds": 8,
    "dataset": {
        "name": "kc_house_data",
        "features": [
            {
                "name": "lat",
                "normalized": true
            },
            {
                "name": "long",
                "normalized": true
            },
            {
                "name": "price",
                "normalized": true,
                "log10": true,
                "out": true
            }
        ]
    }
}
